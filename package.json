{
    "name": "@huijiwiki/mwclient",
    "description": "An opinionated Node.js wrapper for MediaWiki APIs.",
    "version": "0.1.1",
    "packageManager": "yarn@3.3.1",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=16.3"
    },
    "keywords": [
        "mediawiki",
        "huiji",
        "huijiwiki",
        "灰机"
    ],
    "author": "Kurax (https://www.huijiwiki.com)",
    "repository": {
        "type": "git",
        "url": "https://github.com/kurax/huijiwiki-mwclient.git"
    },
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "node": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "rimraf dist && tsc && ts-node-esm build.ts",
        "publish": "yarn build && yarn npm publish --access public"
    },
    "devDependencies": {
        "@types/glob": "^8.0.1",
        "@types/node": "^18.11.18",
        "@types/tough-cookie": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "esbuild": "^0.17.4",
        "eslint": "^8.32.0",
        "glob": "^8.1.0",
        "prettier": "^2.8.3",
        "rimraf": "^4.1.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "chalk": "^5.2.0",
        "formdata-node": "^5.0.0",
        "got": "^12.5.3",
        "tough-cookie": "^4.1.2"
    }
}
